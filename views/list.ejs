
<%- include('header') %>

<!-- // Day 4 -->
  <div class="box" id="heading">
    <h1><%= listItem %></h1>
  </div>
  <div class="box">
    <% for(var i=0; i<item.length ;i++){ %>
    <div class="item">
      <input type="checkbox" />
      <p><%= item[i] %></p>
    </div>
    <%} %>
    <form class="item" method="post" action="/">
      <input type="text" name="newItem" placeholder="New Item" autocomplete="off"/>
      <button type="submit" name="list" value="<%= listItem %>">+</button>
    </form>
  </div>

  <%- include('footer') %>
